# include <stdio.h>
main(Grade)
{
	printf("enter the marks for kids: ");
	
	int marks=0;
	scanf("%d",&marks);
	
	if(marks>=85 && marks<=100)
	{
	   	printf("Grade is A ");
	}
	else if(marks>=70 && marks<=84)
	{
		printf("Grade is B ");
	}
	else if(marks>=55 && marks<=69)
	{
		printf("Grade is C ");
	}
	else if(marks>=40 && marks<=54)
	{
		printf("Grade is D ");
	}
	else if(marks<=40)
	{
		printf("Grade is F ");
	}
}
